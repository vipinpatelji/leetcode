#include <vector>
#include <unordered_map>
using namespace std;

class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map<int,int> mp;
        for (int i = 0; i < nums.size(); i++) {
            int luck = target - nums[i];
            if (mp.find(luck) != mp.end()) {
                return {mp[luck], i};
            }
            mp[nums[i]] = i;
        }
        return {};
    }
};
