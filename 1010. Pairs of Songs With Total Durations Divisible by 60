#include <vector>
using namespace std;

class Solution {
public:
    int numPairsDivisibleBy60(vector<int>& time) {
        vector<int> remainderCount(60, 0);
        int pairCount = 0;
        for (int t : time) {
            int r = t % 60;
            int complement = (60 - r) % 60;
            pairCount += remainderCount[complement];
            remainderCount[r]++;
        }
        return pairCount;
    }
};
