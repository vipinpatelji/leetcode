#include <vector>
#include <unordered_set>

class Solution {
public:
    bool containsDuplicate(std::vector<int>& nums) {
        std::unordered_set<int> mp;
        for (int num : nums) {
            if (mp.count(num)) {
                return true;
            }
            mp.insert(num);
        }
        return false;
    }
};
